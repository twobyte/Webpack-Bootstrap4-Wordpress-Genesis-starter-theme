{
  "name": "tastywebpack",
  "title": "Tasty Web Pack",
  "version": "1.0.0",
  "description": "Starter WordPress child theme webpack builder of Studiopress Genesis Framework by Tasty Digital",
  "keywords": [
    "WordPress",
    "Genesis"
  ],
  "homepage": "http://tastydigital.com/",
  "license": "MIT",
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/twobyte/Webpack-Bootstrap4-Wordpress-Genesis-starter-theme"
  },
  "config": {
    "theme": "tasty-webpack-starter",
    "buildFolder": "dist",
    "proxyURL": "tasty.local",
    "allowedHosts": [
      ".tsty.co",
      ".tasty.local"
    ]
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 version",
      "ie 9"
    ],
    "development": [
      "last 1 version"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "caniuse-lite": "^1.0.30000749",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "imports-loader": "^0.7.1",
    "json-loader": "^0.5.7",
    "modernizr": "^3.6.0",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.8",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "shx": "^0.3.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "fastclick": "^1.0.6",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.14.3",
    "slicknav": "^1.0.8",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "scripts": {
    "clean": "shx rm -rf $npm_package_config_theme/$npm_package_config_buildFolder/ && shx echo 'Themeâ€™s dist folder cleaned'",
    "production": "npm run clean && NODE_ENV=production node_modules/.bin/webpack -p",
    "start": "npm run clean && NODE_ENV=development node_modules/.bin/webpack-dev-server --open",
    "postinstall": "cd $npm_package_config_theme && composer install && cd .. && npm run production"
  }
}
